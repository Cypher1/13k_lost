{
  "name": "js13kserver",
  "version": "1.0.2",
  "description": "js13kgames game jam server",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "clean": "rm -rf dist",
    "build_client": "mergejs -c client_files.txt dist/client.js",
    "build_server": "mergejs -c server_files.txt dist/server.js",
    "build": "npm run clean && mkdir dist && npm run build_server && npm run build_client && cp public/index.html dist/index.html",
    "lint": "eslint ./public --fix"
  },
  "keywords": [
    "js13kgames",
    "server"
  ],
  "engines": {
    "node": "8.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypher1/13k_lost.git"
  },
  "homepage": "https://github.com/voronianski-on-games/js13kGames-boilerplate#readme",
  "devDependencies": {
    "eslint": "^3.3.0",
    "uglify-js": "^3.0.28"
  },
  "dependencies": {
    "express": "4.15.*",
    "socket.io": "2.0.*",
    "glitchd-client-node": "*"
  }
}
